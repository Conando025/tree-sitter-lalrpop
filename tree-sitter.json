{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lalrpop",
      "camelcase": "LaRlPop",
      "title": "lalrpop",
      "scope": "source.lalrpop",
      "file-types": [
        "lalrpop"
      ],
      "injection-regex": "^lalrpop$",
      "class-name": "TreeSitterLalrpop"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A larlpop parser",
    "authors": [
      {
        "name": "Conando",
        "email": "conando025@protonmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Conando025/tree-sitter-lalrpop"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}